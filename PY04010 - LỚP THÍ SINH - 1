class Thisinh:
    def __init__(self,ten,ns,diem1,diem2,diem3):
        self.ten = ten
        self.ns = ns
        self.diem1 = diem1
        self.diem2 = diem2
        self.diem3 = diem3
    def info(self):
        ans = self.diem1 + self.diem2 + self.diem3
        print(f"{self.ten} {self.ns} {ans:.1f}")

a = list()
a.append(input())
a.append(input())
a.append(float(input()))
a.append(float(input()))
a.append(float(input()))
b = Thisinh(a[0],a[1],a[2],a[3],a[4])
Thisinh.info(b)
