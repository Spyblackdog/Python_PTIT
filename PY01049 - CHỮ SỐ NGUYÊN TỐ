from math import sqrt

def check(n):
    if n <= 1:
        return False
    for x in range(2,int(sqrt(n))+1):
        if n % x == 0:
            return False

    return True


for _ in range(int(input())):
    s = input()
    n = len(s)
    if check(n):
        cnt = 0
        for i in range(len(s)):
            if check(int(s[i])):
                cnt += 1
        if cnt > n - cnt :
            print("YES")
        else:
            print("NO")
    else:
        print("NO")
