for _ in range(int(input())):
    s = input()
    st = input()
    n = 0
    cnt = 0
    while s.find(st,n) != -1:
        print(s.find(st,n))
        n = s.find(st,n)+len(st)
        cnt += 1
    print(cnt)
""" By Mai Hong Vu """
