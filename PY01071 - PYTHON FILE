s = input()
if (s[-1] == 'y' or s[-1] == 'Y') and (s[-2] == 'p' or s[-2] == 'P') and (s[-3] == '.'):
    check = True
    for x in range(0,len(s)-3):
        if not (s[x].isalpha() or s[x] == '.' or s[x] == '_'):
            check = False
            break
    if check:
        print('yes')
    else:
        print('no')
else:
    print("no")
