import math

def gcd(a,b):
    if b == 0:
        return a

    return gcd(b,a%b)

def prime(c):
    if c <= 1:
        return False
    for x in range(2,int(math.sqrt(c))+1):
        if c % x == 0:
            return False

    return True

def tinh(d):
    cnt = 0
    for t in str(d):
        cnt += int(t)
    return cnt

for i in range(int(input())):
    n,m = [int(x) for x in input().split()]
    l = gcd(n,m)
    l = tinh(l)
    print("YES") if prime(l) else print("NO")

