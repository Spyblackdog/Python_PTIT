import math

def gcd(a,b):
    if b == 0:
        return a

    return gcd(b,a%b)

def primenumber(n):
    if (n <= 1):
        return False
    for i in range(2,int(math.sqrt(n))+1):
        if n % i == 0:
            return False

    return True

t = int(input())
while t > 0:
    k = 0
    check = False
    N = int(input())
    for x in range(1,N):
        if gcd(x,N) == 1:
            k += 1
    print("YES") if primenumber(k) else print("NO")
    t -= 1
