for _ in range(int(input())):
    n = input()
    if len(set(n)) != 2:
        print("NO")
    else:
        check = True
        for x in range(len(n)-2):
            if n[x] != n[x+2]:
                check = False
                break
        if check:
            print("YES")
        else:
            print("NO")
