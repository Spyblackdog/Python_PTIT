for _ in range(int(input())):
    s = input()
    tong = 0 # Đặt tổng = 0
    tich = 1 # Đặt tích = 0
    for x in range(len(s)): # Duyệt hết chuỗi
        if x % 2 == 0 and int(s[x]) != 0: # Nếu đứng ở vị trí chẵn và khác 0 nhân vào tich
            tich *= int(s[x])
        elif x % 2 != 0:    # Đứng ở vị trí lẻ cộng vào tổng
            tong += int(s[x])
    print(tich,tong)
