for _ in range(int(input())):
    st = input()
    check = 0
    sum1 = 1
    sum2 = 0
    cnt = 0
    for x in range(len(st)):
        if x % 2 == 0:
            check += 1
            if int(st[x]) == 0:
                cnt += 1
            else:
                sum1 *= int(st[x])
        if x % 2 != 0:
            sum2 += int(st[x])
    print(sum1,end = ' ')
    if check == cnt:
        print(0)
    else:
        print(sum2)

""" By Mai Hong Vu """
