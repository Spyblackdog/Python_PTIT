from math import sqrt

def prime(n):
    if n <= 1:
        return False
    for x in range(2,int(sqrt(n)+1)):
        if n % x == 0:
            return False

    return True

N = int(input())
a = list(map(int,input().split()))
b = [False]*N
c = []
for i in range(len(a)):
    if prime(a[i]):
        c.append(a[i])
        b[i] = True
c.sort()
for j in range(len(a)):
    if b[j]:
        print(c[0],end=' ')
        c.pop(0)
    else:
        print(a[j],end=' ')

