from itertools import combinations

n, k = map(int, input().split())  # Đọc N và K
s = list(map(int, input().split()))  # Chuyển input thành danh sách số nguyên
s = sorted(set(s))  # Loại bỏ phần tử trùng và sắp xếp tăng dần

for comb in combinations(s, k):  # Duyệt từng tổ hợp
    print(" ".join(map(str, comb)))  # In tổ hợp ra dạng chuỗi số cách nhau bởi dấu cách
